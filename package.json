{
  "name": "blast-web",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "scss": "node-sass --watch --include-path scss -o assets/scss assets/css --output-style compressed",
    "prefix": "postcss --use autoprefixer --no-map assets/css -d assets/css",
    "test": "npm-run-all -p scss prefix"
  },
  "dependencies": {
    "autoprefixer": "^9.6.0",
    "node-sass": "4.12.0",
    "node-sass-import": "2.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "7.0.17",
    "postcss-cli": "6.1.2"
  },
  "browserslist": [
    "last 2 versions",
    "not ie < 11",
    "Android >= 6",
    "iOS >= 10"
  ]
}
